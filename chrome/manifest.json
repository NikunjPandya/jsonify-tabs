{
  "version": "0.0.0",
  "name": "react-chrome-extension-example",
  "manifest_version": 2,
  "description": "Example for react-chrome-extension-boilerplate",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "c.jpg",
    "128": "c.jpg"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "background",
    "bookmarks",
    "clipboardRead",
    "clipboardWrite",
    "contentSettings",
    "cookies",
    "*://*.google.com/",
    "debugger",
    "history",
    "management",
    "pageCapture",
    "tabs",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],  
  "browser_action": {
    "default_icon": {
      "19": "images/courier_dota.png",
      "38": "images/courier_dota.png"
    },
    "default_title": "dev-manualstore-ext"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
